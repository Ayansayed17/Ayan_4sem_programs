    #include<stdio.h>
    int main(){
    int a,status=0;
    printf("Enter a number:");
    scanf("%d",&a);
    for(i=2;i<a-1;i++){
        if(a%i==0){
            status=1;
            break;
          }
    }    
    if(status==0)
        printf("%d is Prime number.",a);
    else
        printf("%d is not a Prime number.",a);
    return 0;
    }
