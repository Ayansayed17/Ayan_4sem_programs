#include<stdio.h>
void read_array(int a[100], int n){
  for(int i=0;i<n;i++)
      scanf("%d",&a[i]);
}
void display_array(int a[100], int n){
  for(int i=0;i<n;i++)
      printf("%d  ",a[i]);
}
int main()
{
    int n, a[100];
    printf("Enter the size of the array:\n");
    scanf("%d", &n);
    read_array(a, n);
    display_array(a,n);
    return 0;
}
